<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
     android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ImageView
        android:id="@+id/showprocess_imagev"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/ic_launcher"
        android:padding="5dp"
        />
    <TextView
        android:layout_toRightOf="@id/showprocess_imagev"
        android:id="@+id/showprocess_textvname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:text="开发板"
        android:textColor="#000000"
        />
    <TextView
        android:layout_below="@id/showprocess_textvname"
        android:layout_toRightOf="@id/showprocess_imagev"
        android:id="@+id/showprocess_Installsys"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:text="系统应用"
        android:textColor="#000000"
        />


</RelativeLayout>